{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "serve",
			"command": "webdev",
			"args": [
				"serve"
			],
			"type": "shell",
			"group": "build",
			"isBackground": true,
			"problemMatcher": {
				"owner": "custom",
				"pattern": {
					"regexp": "__________"
				},
				"background": {
					"activeOnStart": true,
					"beginsPattern": "^\\[INFO\\] Starting Build",
					"endsPattern": "^\\[INFO\\] Succeeded after"
				}
			}
		}
	]
}